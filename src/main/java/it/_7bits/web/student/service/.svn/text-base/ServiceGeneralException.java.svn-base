package it._7bits.web.student.service;

/**
 * General exception for service layer
 */
public class ServiceGeneralException extends Exception {
    /**
     * General type exception for service layer
     * with custom defined message
     * @param message Custom message
     * @param e Standard exception
     */
    public ServiceGeneralException (final String message, Exception e) {
        super (message, e);
    }
}
